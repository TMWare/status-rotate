{
  "name": "@tmware/status-rotate",
  "version": "1.0.7",
  "description": "Discord Bot Status rotation.",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc -p ./",
    "clean": "rimraf dist",
    "test": "yarn run lint",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "yarn run lint --fix",
    "preversion": "yarn run build && yarn test",
    "prepublishOnly": "yarn run build && yarn test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMWare/status-rotate.git"
  },
  "author": "TMUniversal <me@tmuniversal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMWare/status-rotate/issues"
  },
  "homepage": "https://github.com/TMWare/status-rotate#readme",
  "peerDependencies": {},
  "dependencies": {
    "@tmware/jitsuyo": "^1.4.1",
    "@tmware/variable-parser": "^1.1.2",
    "axios": "0.21.1",
    "discord.js": "^12.5.1",
    "discord-akairo": "^8.1.0"
  },
  "devDependencies": {
    "@tmuniversal/ts": "^1.1.2",
    "@tmware/eslint-config": "^1.0.0",
    "@tmware/prettier-config": "^1.0.1",
    "@types/node": "^14.14.19",
    "eslint": "^7.17.0",
    "husky": "^4.3.6",
    "rimraf": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:fix && yarn run lint",
      "pre-push": "yarn test"
    }
  },
  "keywords": [
    "discord",
    "discord.js",
    "presence",
    "rotate",
    "rotator",
    "status"
  ],
  "prettier": "@tmware/prettier-config"
}
