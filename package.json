{
  "name": "@tmware/status-rotate",
  "version": "1.0.4",
  "description": "Discord Bot Status rotation.",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "commit": "npx git-cz",
    "build": "yarn run clean && tsc -p ./",
    "clean": "rimraf dist",
    "test": "yarn run lint",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore src/",
    "lint:fix": "yarn run lint --fix",
    "preversion": "yarn run build && yarn test",
    "prepublishOnly": "yarn run build && yarn test",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMWare/status-rotate.git"
  },
  "author": "TMUniversal <me@tmuniversal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMWare/status-rotate/issues"
  },
  "homepage": "https://github.com/TMWare/status-rotate#readme",
  "dependencies": {
    "@tmware/jitsuyo": "^1.1.0",
    "@tmware/variable-parser": "^1.1.1",
    "axios": "0.19.2",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.3.1"
  },
  "devDependencies": {
    "@tmuniversal/ts": "^1.0.3",
    "@tmware/prettier-config": "^1.0.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "cz-conventional-changelog": "^3.2.1",
    "eslint": "^7.8.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:fix && yarn run lint",
      "pre-push": "yarn test"
    }
  },
  "keywords": [
    "discord",
    "status",
    "presence",
    "rotate",
    "rotator"
  ]
}
