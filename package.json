{
  "name": "@tmware/status-rotate",
  "version": "1.1.1",
  "description": "Discord Bot Status rotation.",
  "main": "dist/index.js",
  "types": "typings/index.d.ts",
  "exports": {
    "require": "./dist/index.js",
    "import": "./index.esm.mjs"
  },
  "files": [
    "dist",
    "index.esm.mjs",
    "src",
    "typings"
  ],
  "scripts": {
    "build": "yarn run clean && tsc -p ./",
    "clean": "rimraf dist",
    "test": "yarn run lint src/",
    "lint": "eslint --cache --report-unused-disable-directives --ext .js,.ts --ignore-path .gitignore --ignore-path .eslintignore",
    "lint:fix": "yarn run lint src/ --fix",
    "preversion": "yarn run build && yarn test",
    "prepublishOnly": "pinst --disable && yarn run build && yarn test",
    "postpublish": "pinst --enable",
    "postversion": "git push && git push --tags",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TMWare/status-rotate.git"
  },
  "author": "TMUniversal <me@tmuniversal.eu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TMWare/status-rotate/issues"
  },
  "funding": [
    {
      "type": "patreon",
      "url": "https://www.patreon.com/bePatron?u=19830329"
    },
    {
      "type": "ko-fi",
      "url": "https://ko-fi.com/tmuniversal"
    },
    {
      "type": "github sponsors",
      "url": "https://github.com/sponsors/TMUniversal"
    }
  ],
  "homepage": "https://github.com/TMWare/status-rotate#readme",
  "peerDependencies": {},
  "dependencies": {
    "@tmware/jitsuyo": "^1.4.1",
    "@tmware/variable-parser": "^1.1.2",
    "axios": "0.21.1",
    "discord-akairo": "^8.1.0",
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "@tmuniversal/ts": "^1.1.2",
    "@tmware/eslint-config": "^1.1.2",
    "@tmware/eslint-config-typescript": "^1.0.5",
    "@types/node": "^14.14.31",
    "eslint": "^7.20.0",
    "husky": "^5.1.0",
    "pinst": "^2.1.4",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "discord",
    "discord.js",
    "presence",
    "rotate",
    "rotator",
    "status"
  ]
